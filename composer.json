{
    "name": "kenlog/doc-pht",
    "type": "project",
    "description": "Clipboard generator for code snippets",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentino Pesce",
            "email": "valentino@iltuobrand.it",
            "role": "Developer"
        },
        {
            "name": "Craig Crosby",
            "email": "creecros@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "nezamy/route": "^1.2",
        "symfony/serializer": "^4.2",
        "symfony/property-access": "^4.2",
        "bocharsky-bw/arrayzy": "^0.6.1",
        "phpunit/phpunit": "^8.1",
        "symfony/translation": "^4.3",
        "nette/forms": "^3.0",
        "emojione/emojione": "^4.5",
        "tracy/tracy": "^2.6"
    },
    "autoload": {
        "psr-4": {
          "":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "": "tests/" }
    },
    "config": {
    	"optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
